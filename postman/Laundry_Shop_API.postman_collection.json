        {
          "name": "Sign In",
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "let jsonData = {};",
                  "try { jsonData = pm.response.json(); } catch (e) {}",
                  "const at = jsonData.accessToken || jsonData.data?.accessToken || jsonData.tokens?.accessToken;",
                  "const rt = jsonData.refreshToken || jsonData.data?.refreshToken || jsonData.tokens?.refreshToken;",
                  "if (at) pm.collectionVariables.set('accessToken', at);",
                  "if (rt) pm.collectionVariables.set('refreshToken', rt);"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"customer1@example.com\",\n  \"password\": \"Passw0rd!\"\n}"
